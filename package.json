{
  "name": "yafrontendblog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "start:mock": "concurrently --names \"WEB,SERVER\" -c \"green,blue\" \"npm:mock\" \"npm:mock:server\"",
    "mock": "cross-env NODE_ENV=mock parcel src/index.html",
    "build": "parcel build src/index.html --no-source-maps",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "mock:server": "json-server --no-cors mock/db.js --routes mock/routes.json"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.4",
    "date-fns": "^1.30.1",
    "firebase": "^5.8.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "ts-domain": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^1.4.0",
    "faker": "^4.1.0",
    "json-server": "^0.14.2",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^1.16.4",
    "sass": "^1.17.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
